{
	"name": "Jupyter",

	"dockerComposeFile": [
		"docker-compose.yml",
	],

	"service": "jupyterlab",

	"workspaceFolder": "/home/jovyan",

	"extensions": [
		"ms-python.python",
		"ms-toolsai.jupyter",
		"ms-toolsai.jupyter-keymap",
		"redhat.vscode-yaml",
		"ms-kubernetes-tools.vscode-kubernetes-tools"
	],

	"settings": {
		"python.defaultInterpreterPath": "/opt/conda/bin/python"
		"vs-kubernetes": {
			"vscode-kubernetes.helm-path.linux": "/usr/local/bin/helm",
			"vscode-kubernetes.kubectl-path.linux": "/usr/local/bin/kubectl",
			"vscode-kubernetes.minikube-path.linux": "/usr/bin/minikube"
		},
	},

	"remoteUser": "jovyan",
}